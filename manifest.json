{
  "manifest_version": 3,
  "name": "bloggyai",
  "version": "1.0",
  "description": "A web scraper extension",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ]
}